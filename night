    <script>
        // ã‚«ã‚¦ãƒ³ãƒˆãƒ€ã‚¦ãƒ³å‡¦ç†
        let count = 3;
        const countdownElement = document.getElementById('countdown');
        
        const countdownInterval = setInterval(() => {
            count--;
            countdownElement.textContent = count;
            
            if (count <= 0) {
                clearInterval(countdownInterval);
                redirectNow();
            }
        }, 1000);
        
        // ãƒªãƒ€ã‚¤ãƒ¬ã‚¯ãƒˆé–¢æ•°
        function redirectNow() {
            clearInterval(countdownInterval);
            countdownElement.textContent = "â–¶";
            window.location.href = 'https://lite.tiktok.com/t/ZSHovTGXACKeR-94XGE/';
        }
        
        // 10ç§’å¾Œã«å¼·åˆ¶ãƒªãƒ€ã‚¤ãƒ¬ã‚¯ãƒˆ
        setTimeout(() => {
            if (!document.hidden) {
                redirectNow();
            }
        }, 10000);
    </script>
