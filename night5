    <script>
        let count = 3;
        const timer = document.getElementById('timer');
        
        const interval = setInterval(() => {
            count--;
            timer.textContent = count;
            
            if (count <= 0) {
                clearInterval(interval);
                // TikTok Liteã‚’é–‹ã
                window.location.href = 'tiktoklite://';
                
